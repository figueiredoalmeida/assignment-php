{
    "name": "lokalise/assignment-php",
    "description": "Lokalise homework for PHP developers",
    "type": "project",
    "authors": [
        {
            "name": "Rodrigo de Almeida",
            "email": "figueiredoalmeida@gmail.com"
        },
        {
            "name": "Arturs Ziborovs",
            "email": "arthur@lokalise.com"
        }
    ],
    "minimum-stability": "stable",
    "require": {
        "php": "^7.4.8",
        "ext-ctype": "*",
        "ext-iconv": "*",
        "ext-json": "*",
        "composer/package-versions-deprecated": "1.11.99.3",
        "doctrine/doctrine-bundle": "^2.4",
        "doctrine/doctrine-migrations-bundle": "^3.1",
        "doctrine/orm": "^2.9",
        "lexik/jwt-authentication-bundle": "^2.12",
        "nelmio/api-doc-bundle": "^4.5",
        "sensio/framework-extra-bundle": "^6.1",
        "symfony/asset": "4.4.*",
        "symfony/console": "4.4.*",
        "symfony/dotenv": "4.4.*",
        "symfony/flex": "^1.3.1",
        "symfony/framework-bundle": "4.4.*",
        "symfony/property-info": "4.4.*",
        "symfony/proxy-manager-bridge": "4.4.*",
        "symfony/twig-bundle": "4.4.*",
        "symfony/validator": "4.4.*",
        "symfony/yaml": "4.4.*",
        "doctrine/annotations": "^1.0",
        "phpdocumentor/reflection-docblock": "^5.2",
        "symfony/property-access": "4.4.*",
        "symfony/serializer": "4.4.*",
        "symfony/phpunit-bridge": "^5.3",
        "symfony/monolog-bundle": "^3.7",
        "nelmio/cors-bundle": "^2.1",
        "symfony/expression-language": "4.4.*",
        "symfony/security-bundle": "4.4.*"
    },
    "require-dev": {
        "symfony/maker-bundle": "^1.33",
        "phpunit/phpunit": "^9"
    },
    "autoload": {
        "psr-4": {
            "App\\": "src/"
        }
    },
    "autoload-dev": {
        "psr-4": {
            "App\\Tests\\": "tests/"
        }
    },
    "scripts": {
        "auto-scripts": {
            "cache:clear": "symfony-cmd",
            "assets:install %PUBLIC_DIR%": "symfony-cmd"
        }
    }
}
